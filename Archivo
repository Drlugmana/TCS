private async Task ProcessIncidentData(CancellationToken stoppingToken)
{
    List<Incident> incidents = await GetListIncidentsAsync(stoppingToken);

    // ðŸ”¹ Cambia estas dos fechas segÃºn lo que necesites
    DateTime fechaInicio = new DateTime(2025, 10, 1); // ejemplo: 1 de octubre 2025
    DateTime fechaFin = new DateTime(2025, 10, 16);   // ejemplo: 16 de octubre 2025

    // ðŸ”¹ Filtra los incidentes que estÃ©n dentro del rango de fechas
    incidents = incidents
        .Where(inc => DateTime.TryParse(inc.Fecha_de_Abierto, out DateTime fecha)
                   && fecha >= fechaInicio && fecha <= fechaFin)
        .ToList();

    _logger.LogInformation($"Se han detectado {incidents.Count} incidentes entre {fechaInicio:yyyy-MM-dd} y {fechaFin:yyyy-MM-dd}");

    await UpdateData(incidents);
    _logger.LogInformation("âœ… Se han actualizado los incidentes del rango indicado");
}