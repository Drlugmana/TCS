Med_Aux_CPU_USED =
VAR maxdate =
    MAX ( CP_Clusteres_de_VMware[IndiceMes] )
VAR valor =
    CALCULATE (
        SUM ( CP_Clusteres_de_VMware[Aux_CPU_Total_USED] ),
        CP_Clusteres_de_VMware[IndiceMes] = maxdate
    )
VAR maxvalor =
    CALCULATE (
        SUM ( CP_Clusteres_de_VMware[Aux_CPU_Total] ),
        CP_Clusteres_de_VMware[IndiceMes] = maxdate
    )
VAR ratio = DIVIDE ( valor, maxvalor )
RETURN
    MIN ( ratio, 1 )