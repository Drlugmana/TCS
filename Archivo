private static string NormalizeName(string name)
{
    return (StripDomain(name ?? string.Empty).Trim()).ToUpperInvariant();
}

// Quitar de "No Monitoreados CMDB" los que ya est√°n en "Monitoreados"
var monitoredNames = new HashSet<string>(
    _serversWithEntities
        .Where(x => x?.Server?.Name != null)
        .Select(x => NormalizeName(x.Server.Name)),
    StringComparer.OrdinalIgnoreCase
);

// Filtra y de paso elimina duplicados por nombre normalizado
_serversNoMatch = _serversNoMatch
    .Where(s => s?.Name != null && !monitoredNames.Contains(NormalizeName(s.Name)))
    .GroupBy(s => NormalizeName(s.Name))
    .Select(g => g.First())
    .ToList();