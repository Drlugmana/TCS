// Inside MLPredictor.cs (scope interno)
class IncidentData
{
    public string ProblemTitle { get; set; } = string.Empty;
    public bool EsTCS { get; set; }   // etiqueta del training (para evaluaci√≥n si cargases dataset)
}

class IncidentPrediction : IncidentData
{
    [ColumnName("PredictedLabel")]
    public bool PredictedEsTCS { get; set; }
    public float Probability { get; set; }
    public float Score { get; set; }
}